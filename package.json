    "eslint-plugin-import": "^2.7.0",
    "jsdoc": "^3.5.4",
    "lab": "^13.0.4",
    "minami": "^1.2.3",
    "node-sass": "^3.13.0",
    "nodemon": "^1.11.0",
    "sass-loader": "^6.0.3",
    "should": "^11.2.1",
