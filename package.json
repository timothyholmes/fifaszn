    "axios": "^0.16.2",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "bluebird": "^3.0",
    "bootstrap": "^3.3.7",
    "classnames": "^2.2.5",
    "concurrently": "^3.4.0",
    "good": "^7.0.1",
    "good-console": "^6.1.2",
    "good-squeeze": "^4.0.0",
    "hapi": "^14.2.0",
    "inert": "^4.0.2",
    "joi": "^9.0.4",
    "lodash": "^4.17.2",
